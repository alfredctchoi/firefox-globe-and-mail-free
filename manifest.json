{

  "manifest_version": 2,
  "name": "Globe and Mail Free",
  "version": "1.0",
  "description": "Read the Globe and Mail for free",
  "permissions": [
    "<all_urls>",
    "webRequestBlocking"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ]

}